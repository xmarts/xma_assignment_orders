        <record id="ba_sale_order_form_inherit" model="ir.ui.view">
            <field name="name">ba_sale_order_form_inherit</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">

            <field name="order_line" position="after">
              <button name="save_order" string="Reservar y actualizar" type="object" class="oe_highlight"/>

            </field>
            <field name="order_line" position="before">
              <button name="save_order" string="Reservar y actualizar" type="object" class="oe_highlight"/>
            </field>
            
            <xpath expr="//field[@name='order_line']/tree/field[@name='name']" position="after">
              <field name="quantity_unit" />
              <field name="grados_alcohol" invisible="1"/>
              <field name="contenido" invisible="1" />
              <field name="pricelist_id" invisible="1" />
              <field name="pricelist_name" />
              <field name="unidad"  />
              <field name="sub_unidad" />
            </xpath>
            <xpath expr="//field[@name='order_line']/tree/field[@name='product_uom_qty']" position="replace">
              <field name="product_uom_qty" invisible="True"/>
              <field name="cantidad" readonly="1"/>
            </xpath>
            <xpath expr="//field[@name='order_line']/tree/field[@name='product_uom']" position="replace">
              <field name="unidades_sin_reserva" invisible="1"/>
              <field name="product_uom" invisible="1"/>
              <field name="inv_available" string="Inv.Disponible" attrs="{'column_invisible': [('parent.state', 'in', ['sale', 'done'])]}"/>
            </xpath>
            <xpath expr="//field[@name='order_line']/tree/field[@name='tax_id']" position="replace">
              <field name="tax_id" invisible="1"/>
            </xpath>
            <xpath expr="//field[@name='order_line']/tree/field[@name='price_unit']" position="replace">
              <field name="box_price" readonly="1" force_save="1" />
              <field name="price_unit" readonly="1" force_save="1" />
            </xpath>
            <xpath expr="//field[@name='order_line']/tree/field[@name='price_subtotal']" position="after">
              <field name="price_subtotal_base" readonly="1"/>
            </xpath>

            <field name="note" position = "after">
                <field name="taxes_by_group" >
                    <tree create="false" edit="false">
                        <field name="name" />
                        <field name="monto_base" />
                        <field name="total_impuesto" />
                    </tree>
                </field>
            </field>

            <field name="validity_date" position="before" >
              <field name="serie_id" options="{'no_create': True, 'no_create_edit':True}" domain="[('relation_id.model', '=','sale.order'),('company_id', '=',company_id)]" required="1"/>
            </field>

            <label for="pricelist_id" position="replace">
              <label for="pricelist_id" string="Lista de precios" groups="product.group_product_pricelist"/>
            </label>
            <field name="pricelist_id" position="replace">
                <field name="pricelist_id" readonly="1" />
            </field>

          </field>
        </record>

        <delete id="sale.menu_product_pricelist_main" model="ir.ui.menu"/>
        <menuitem id="menu_product_pricelist_main" name="Listas de precios" parent="sale.product_menu_catalog" action="ba_synchronize.product_pricelist_action2" groups="product.group_product_pricelist" sequence="3"/>

</odoo>